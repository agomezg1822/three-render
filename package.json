{
  "name": "three-seed",
  "version": "0.4.0",
  "description": "Three.js starter project boilerplate bundled with webpack",
  "keywords": [
    "webgl",
    "boilerplate",
    "three.js",
    "es6",
    "webpack"
  ],
  "scripts": {
    "start": "webpack serve",
    "prebuild": "rimraf ./docs && mkdir ./docs",
    "build": "webpack --mode production",
    "test": "jest"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(glb|jpg|jpeg|png|gif|svg)$": "<rootDir>/fileMock.js"
    },
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@types/jest": "^29.5.12",
    "node-fetch": "^3.3.2",
    "three": "^0.147.0",
    "webpack-cli": "^5.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^7.0.0",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^4.11.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
